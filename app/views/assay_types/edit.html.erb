<h1>Edit <%= t('assays.assay').downcase %> type</h1>

<div class="show_basic">
  <div style="float:left">
    <ul><%= ontology_editor_display AssayType, false, false, @assay_type.id %></ul>
  </div>      
  <div style="float:left">
    <%= form_for @assay_type do |f| %>
      <%= render :partial => "form", :locals => { :f => f, :action=>:edit } -%>
    <% end -%>
  </div>
</div>