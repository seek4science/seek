<% is_modal ||= false %>
<div class='panel panel-default'>
  <div class='panel-heading'>
    License and permissions
  </div>
  <div class='panel-body'>
    <%= render :partial => 'assets/license_selector', locals: {resource: @data_file} %>
    <%= render :partial => 'sharing/form' -%>
    <%= render :partial => 'assets/author_form', locals: { form: f, collapsed: true } %>
     <%if(is_modal)%>
      <a class=" btn btn-default" onClick="location.reload();">Cancel</a>
    <%else%>
      <%= cancel_button(data_files_path) -%>
    <%end%>
    <div class='multi-step-buttons'>
      <%= multi_step_start_button %>
      <%= multi_step_back_button %>
      <%= multi_step_forward_button %>
      <%= multi_step_end_button %>
    </div>
  </div>
</div>
<%= wizard_footer_tips %>