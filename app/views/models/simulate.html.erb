<% content_for(:buttons) { button_link_to("Back to #{t('model')}", 'back', model_path(@model,:version=>@display_model.version, :code=>params[:code])) } %>

<%= render :partial => "general/item_title",:locals=>{:item=>@model,:title_postfix=>" - JWS Online #{t('model')} Simulation"} %>

<br/>

<% if @jws_error %>
  <%= alert_box('danger', :id => 'error_flash', :class => 'fade in') do %>
    <%= @jws_error %>
  <% end %>
<% else %>
  <% if @constraint_based %>
    <div id="jws_simulator_wrapper">
      <iframe src="<%= @simulate_url.html_safe -%>" id="jws_simulator" width="100%" height="1350px" frameborder=0>You need
        a Frames Capable browser to view this content.
      </iframe>
    </div>
  <% else %>
    <%= render 'simulate_select_constraint_based' %>
  <% end %>
<% end %>



