    <!-- http://jermontology.org/ontology/JERMOntology#Affinity_chromatography -->

    <owl:Class rdf:about="http://jermontology.org/ontology/JERMOntology#Affinity_chromatography">
        <rdfs:subClassOf rdf:resource="http://jermontology.org/ontology/JERMOntology#Chromatography"/>
        <dc:description>Affinity chromatography is based on selective non-covalent interaction between an analyte and specific molecules.
 It is often used in biochemistry in the purification of proteins bound to tags. These fusion proteins are labeled with compounds such as His-tags, biotin or antigens, which bind to the stationary phase specifically.</dc:description>
        <dc:identifier>JERM:00110</dc:identifier>
        <rdfs:comment>affinity_purification</rdfs:comment>
    </owl:Class>
    


