    </owl:Class>
    


    <!-- http://jermontology.org/ontology/JERMOntology#Assay -->

    <owl:Class rdf:about="http://jermontology.org/ontology/JERMOntology#Assay">
        <rdfs:subClassOf rdf:resource="http://jermontology.org/ontology/JERMOntology#process"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://jermontology.org/ontology/JERMOntology#hasInput"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://jermontology.org/ontology/JERMOntology#Data_sample"/>
                            <rdf:Description rdf:about="http://jermontology.org/ontology/JERMOntology#Sample"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://jermontology.org/ontology/JERMOntology#hasOutput"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://jermontology.org/ontology/JERMOntology#Data"/>
                            <rdf:Description rdf:about="http://jermontology.org/ontology/JERMOntology#Data_sample"/>
                            <rdf:Description rdf:about="http://jermontology.org/ontology/JERMOntology#Sample"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://jermontology.org/ontology/JERMOntology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://jermontology.org/ontology/JERMOntology#Asset"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://jermontology.org/ontology/JERMOntology#hasType"/>
                <owl:someValuesFrom rdf:resource="http://jermontology.org/ontology/JERMOntology#Assay_type"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://jermontology.org/ontology/JERMOntology#hasType"/>
                <owl:someValuesFrom rdf:resource="http://jermontology.org/ontology/JERMOntology#Technology_type"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://jermontology.org/ontology/JERMOntology#investigates"/>
                <owl:someValuesFrom rdf:resource="http://jermontology.org/ontology/JERMOntology#Organism"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://jermontology.org/ontology/JERMOntology#isAssociatedWith"/>
                <owl:someValuesFrom rdf:resource="http://jermontology.org/ontology/JERMOntology#Protocol"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://jermontology.org/ontology/JERMOntology#isPartOf"/>
                <owl:someValuesFrom rdf:resource="http://jermontology.org/ontology/JERMOntology#Study"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://jermontology.org/ontology/JERMOntology#Investigation"/>
        <owl:disjointWith rdf:resource="http://jermontology.org/ontology/JERMOntology#Study"/>
        <dc:description>An assay is a combination of a process (experiment) and a protocol.</dc:description>
        <dc:identifier>JERM:00021</dc:identifier>
    </owl:Class>
    


    <!-- http://jermontology.org/ontology/JERMOntology#Assay_type -->

    <owl:Class rdf:about="http://jermontology.org/ontology/JERMOntology#Assay_type">
        <rdfs:subClassOf rdf:resource="http://jermontology.org/ontology/JERMOntology#Experimental_factors"/>
        <owl:disjointWith rdf:resource="http://jermontology.org/ontology/JERMOntology#Technology_type"/>
        <dc:description>An assay type describes the type of experiment being performed</dc:description>
        <dc:identifier>JERM:00024</dc:identifier>
        <rdfs:comment>The assay_type hierarchy allows scientists to categorise their experimental assays, informatics analyses and computational models within SysMO</rdfs:comment>
    </owl:Class>
    


    <!-- http://jermontology.org/ontology/JERMOntology#Asset -->

    <owl:Class rdf:about="http://jermontology.org/ontology/JERMOntology#Asset">
        <rdfs:subClassOf rdf:resource="http://jermontology.org/ontology/JERMOntology#information_entity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://jermontology.org/ontology/JERMOntology#hasContributor"/>
                <owl:someValuesFrom rdf:resource="http://jermontology.org/ontology/JERMOntology#Person"/>
            </owl:Restriction>
